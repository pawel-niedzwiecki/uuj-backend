{
  "kind": "collectionType",
  "collectionName": "ratings",
  "info": {
    "singularName": "rating",
    "pluralName": "ratings",
    "displayName": "Rating"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "rating": {
      "type": "decimal",
      "required": true,
      "default": 5,
      "max": 5,
      "min": 1
    },
    "author": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service",
      "mappedBy": "rating"
    }
  }
}
