{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "news",
        "services"
      ],
      "required": true
    },
    "content": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "category"
    },
    "news": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::new.new",
      "mappedBy": "category"
    }
  }
}
